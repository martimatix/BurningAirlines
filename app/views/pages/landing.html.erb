  <a href="#">Home</a>

  <div id="main"></div>

  <template id="flightView-template">
    <h2>List of flights</h2>
    <table>
      <tr>
        <th>Flight Number</th>
        <th>Date</th>
        <th>Origin</th>
        <th>Destination</th>
        <th>Seats Available</th>
      </tr>
      <div id="table_data"></div>
    </table>
  </template>

  <template id="flightListView-template">
    <tr>
      <td>{{ flight_num }}</td>
      <td>{{ date }}</td>
      <td>{{ origin }}</td>
      <td>{{ destination }}</td>
      <td></td>
    </tr>
  </template>

  <template id="flightSearch-template">
    <h2>Search for Available Flights</h2>
    <form>
      <div>
        <label for="origin">Origin:</label>
        <input id="flight-origin">
      </div>
      <div>
        <label for="destination">Destination:</label>
        <input id="flight-destination">
      </div>
      <button>Search for Flights</button>
    </form>
  </template>

